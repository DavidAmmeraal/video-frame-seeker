{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-underscore-dangle": [2, { "allowAfterThis": true }]
  },
  "settings": {
      "import/resolver": {
          "webpack": {
              "config": "config/webpack.config.js"
          }
      }
  }
}
